#%RAML 1.0
title: CoinGlass API v4.0 Complete Specification
version: v4
baseUri: https://open-api-v4.coinglass.com
mediaType: application/json
description: |
  Complete cryptocurrency market data API covering all 100+ endpoints for futures, spot, options, ETF, 
  on-chain, and advanced market indicators from CoinGlass.

securitySchemes:
  apiKey:
    description: API Key authentication
    type: Pass Through
    describedBy:
      headers:
        CG-API-KEY:
          description: Your CoinGlass API key
          type: string
          required: true

types:
  # Common Response Types
  StandardResponse:
    type: object
    properties:
      code:
        type: string
        description: Response code (0 indicates success)
        example: "0"
      msg:
        type: string
        description: Response message
        example: "success"
      data:
        type: any
        description: Response payload

  ErrorResponse:
    type: object
    properties:
      code:
        type: string
        description: Error code
      msg:
        type: string
        description: Error message

  # Time Series Data Types
  OHLCData:
    type: object
    properties:
      time:
        type: integer
        format: int64
        description: Timestamp in milliseconds
      open:
        type: number
        description: Opening value
      high:
        type: number
        description: High value
      low:
        type: number
        description: Low value
      close:
        type: number
        description: Closing value

  TimeValueData:
    type: object
    properties:
      time:
        type: integer
        format: int64
        description: Timestamp in milliseconds
      value:
        type: number
        description: Data value

  # Market Data Types
  MarketData:
    type: object
    properties:
      symbol:
        type: string
      price:
        type: number
      price_change:
        type: number
      price_change_percent:
        type: number
      volume:
        type: number
      open_interest:
        type: number
      funding_rate:
        type: number

  # Common Query Parameters
  CommonTimeParams:
    type: object
    properties:
      startTime:
        type: integer
        format: int64
        required: false
        description: Start timestamp in milliseconds
      endTime:
        type: integer
        format: int64
        required: false
        description: End timestamp in milliseconds
      limit:
        type: integer
        required: false
        description: Number of results (max varies by endpoint)
        default: 100

# Root endpoint
/:
  displayName: API Root

# Futures Market Endpoints
/api/futures/supported-coins:
  displayName: Futures Supported Coins
  description: |
    Get all supported futures coins  
    **Cache:** Every 1 minute  
    **Plan Availability:** All plans  
    **Example cURL:**  
    ```shell
    curl --request GET "https://open-api-v4.coinglass.com/api/futures/supported-coins"
    ```
  securedBy: [apiKey]
  get:
    responses:
      200:
        body:
          application/json:
            type: StandardResponse
            example: |
              {
                "code": "0",
                "msg": "success",
                "data": [
                  "BTC",
                  "ETH",
                  "SOL",
                  "XRP"
                ]
              }

/api/futures/supported-exchange-pairs:
